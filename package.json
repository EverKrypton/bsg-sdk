{
  "name": "@manekiswap/sdk",
  "license": "MIT",
  "version": "1.0.7",
  "description": "ðŸ›  An SDK for building applications on top of Manekiswap",
  "files": [
    "abis",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manekiswap/sdk.git"
  },
  "keywords": [
    "manekiswap",
    "ethereum"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "generate-abis": "ts-node scripts/abis.ts",
    "compile-external-abi-types": "typechain --target ethers-v5 --out-dir abis/types './abis/**/*.json'",
    "clean": "rimraf dist",
    "lint": "eslint --fix --ext .ts",
    "build": "yarn clean && yarn compile-external-abi-types && rollup -c",
    "start": "rollup -c -w",
    "test": "jest"
  },
  "dependencies": {
    "big.js": "6.1.1",
    "decimal.js-light": "2.5.1",
    "ethers": "5.4.7",
    "jsbi": "3.2.4",
    "tiny-invariant": "1.1.0",
    "tiny-warning": "1.0.3",
    "toformat": "2.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "20.0.0",
    "@rollup/plugin-node-resolve": "13.0.4",
    "@rollup/plugin-typescript": "8.2.5",
    "@typechain/ethers-v5": "7.1.0",
    "@types/big.js": "6.1.2",
    "@types/jest": "27.0.1",
    "@types/node": "16.9.4",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "jest": "27.2.0",
    "prettier": "2.4.1",
    "rimraf": "3.0.2",
    "rollup": "2.56.3",
    "rollup-plugin-terser": "7.0.2",
    "ts-jest": "27.0.5",
    "ts-node": "10.2.1",
    "tslib": "2.3.1",
    "typechain": "5.1.2",
    "typescript": "4.4.3"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "bugs": {
    "url": "https://github.com/manekiswap/sdk/issues"
  },
  "homepage": "https://github.com/manekiswap/sdk#readme"
